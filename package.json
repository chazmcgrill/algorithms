{
    "name": "algorithms",
    "version": "1.0.0",
    "description": "Collection of algorithms and data structures",
    "main": "index.js",
    "scripts": {
        "check-types": "npx tsc",
        "format": "npm run prettier -- --write",
        "lint": "eslint . --ext .ts",
        "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts)\"",
        "start": "tsc --watch",
        "test:watch": "jest --verbose --watch",
        "test": "jest",
        "validate": "npm run lint && npm run check-types"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chazmcgrill/algorithms.git"
    },
    "keywords": [
        "typescript",
        "algorithms",
        "data-structures"
    ],
    "author": "Charlie Taylor <mail@charlietaylorcoder.com> (https://charlietaylorcoder.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/chazmcgrill/algorithms/issues"
    },
    "homepage": "https://github.com/chazmcgrill/algorithms#readme",
    "devDependencies": {
        "@types/jest": "^26.0.7",
        "@types/node": "^14.0.26",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^26.1.0",
        "prettier": "^2.5.1",
        "ts-jest": "^26.1.3",
        "typescript": "^4.5.4"
    }
}
